  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.40",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "axios": "^0.24.0",
    "concurrently": "^5.3.0",
    "electron": "^9.4.4",
    "electron-builder": "^22.14.5",
    "electron-is-dev": "^1.2.0",
    "electronmon": "^1.1.2",
    "fs-extra": "^10.0.0",
    "got": "^12.0.1",
    "jsdom": "^19.0.0",
    "node-stream-zip": "^1.15.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^3.4.1",
    "sass": "^1.45.2",
    "typescript": "^3.9.10",
    "unrar-promise": "^2.0.1",
    "wait-on": "^5.3.0"
  },
  "scripts": {
    "start": "concurrently \"npm run react-serve\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "build": "npm run react-build && npm run electron-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "npm run react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "SET BROWSER=none&&npm run react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron-main.cjs",
  "homepage": "./",
  "devDependencies": {
    "jest": "^27.4.7"
  }
